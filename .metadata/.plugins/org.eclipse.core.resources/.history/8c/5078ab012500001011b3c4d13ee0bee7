package Modelo;

public class Juegos extends ElementosMultimedia{


	public Juegos(int id, String ubicacion, String genero, String nombre,int numeroFases) {
		super(id, ubicacion, genero, nombre);
		this.numFases = numFases;	}

	private int numFases;

	public int getNumFases() {
		return numFases;
	}

	public void setNumFases(int numFases) {
		this.numFases = numFases;
	}


	@Override
	public boolean estaDisponible() {
		// TODO Auto-generated method stub
		return true;
	}

	@Override
	public int getDevuelveDiasPrestamo() {
		// TODO Auto-generated method stub
		return 10;
	}
	
}
